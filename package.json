        "bn-ng-idle": "^2.0.4",
        "bootstrap": "^5.2.2",
        "core-js": "3.25.5",
        "events": "^3.3.0",
        "localstorage-polyfill": "^1.0.1",
        "lodash": "^4.17.21",
        "lottie-web": "^5.7.11",
        "moment": "^2.29.4",
