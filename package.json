{
    "name": "cro-website",
    "version": "0.0.2",
    "license": "MIT",
    "type": "module",
    "scripts": {
        "ng": "ng",
        "serve": "node server",
        "build": "ng build",
        "build-prod": "ng build --configuration production",
        "build-dev": "ng build --configuration development",
        "dev": "node --max-old-space-size=8192 ./node_modules/@angular/cli/bin/ng build && ng serve",
        "lint": "npx eslint --max-warnings=0 './**/*.{js,ts}' && prettier --check '*.{json,js}' './**/*.{js,ts}'",
        "lint:fix": "npm run lint --fix",
        "prettier": "npx prettier . --check",
        "prettier:fix": "npx prettier './**/*.{js,ts}' --write",
        "format": "npm run prettier:fix && npm run lint:fix",
        "e2e": "ng e2e",
        "test": "ngcc && ng test --watch=false"
    },
    "private": true,
    "dependencies": {
